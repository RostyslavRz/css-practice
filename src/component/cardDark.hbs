{{#with data}}
<div class="card">
    <div class="card_image" style="background-image: url({{photo}}"></div>
    {{#if isNew}}<span class="badge">NEW</span>{{/if}}
    <strong class="card_author">{{name}}</strong>
    <p class="card_text">{{description}}</p>
    <button class="card_button">Read All</button>
</div>
{{/with}}